<?xml version="1.0" encoding="utf-8"?>
<e:Skin xmlns:e="http://ns.egret-labs.org/egret" xmlns:w="http://ns.egret-labs.org/wing"
        minHeight="60" minWidth="140">
	<w:HostComponent name="egret.gui.Button" />
	<e:states>
		<e:State name="up" />
		<e:State name="down" />
		<e:State name="disabled" />
	</e:states>
	<e:UIAsset id="UIA_bg" width="100%" height="100%" source="ly_shop_5_png"
	           source.down="ly_shop_5_png" source.disabled="ly_shop_5_png" />
	<e:Group left="10" right="10" top="10" bottom="10">
		<e:layout>
			<e:HorizontalLayout gap="0" verticalAlign="middle" horizontalAlign="center"/>
		</e:layout>
		<e:UIAsset id="iconDisplay"/>
		<e:Label id="labelDisplay" verticalAlign="middle"  paddingLeft="5" paddingRight="5"
		         textAlign="center" textColor="0xFFFFFF"
		         textColor.down="0xf1dbd7" textColor.disabled="0xcccccc" text="222" fontFamily="微软雅黑" size="32" width="240" height="90"/>
	</e:Group>
